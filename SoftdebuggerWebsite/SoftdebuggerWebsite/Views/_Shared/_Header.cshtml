@using Newtonsoft.Json.Linq;

@{
    string LanguageText = Convert.ToString(Session["LangFile"]);
    JObject objLanguagerequiredText = JObject.Parse(LanguageText);

}

<meta charset="utf-8">
<!--[if IE]><meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1"><![endif]-->
<meta name="viewport" content="width=device-width, initial-scale=1.0">

@*<title>@objLanguagerequiredText.SelectToken("TitleKey").ToString()</title>*@
<meta name="description" content="" />
<!-- templatemo 411 dragonfruit -->
<meta name="author" content="softdebugger">
<!-- Favicon-->
<link rel="shortcut icon" href="./favicon.png" />
<!-- Font Awesome -->
<link href="../css/font-awesome.min.css" rel="stylesheet">
<!-- Bootstrap -->
<link href="../css/bootstrap.min.css" rel="stylesheet">
<!-- Camera -->
<link href="../css/camera.css" rel="stylesheet">

<!-- Template  -->
<link href="../css/templatemo_style.css" rel="stylesheet">
<link href="~/css/style.css" rel="stylesheet" />

<!--Add Jquery Reference-->
<script src="../js/jquery-3.2.1.js"></script>

<div id="templatemo_mobile_menu">
    <ul class="nav nav-pills nav-stacked">
        <li><a  class="homepage"><i class="glyphicon glyphicon-home"></i>&nbsp; @objLanguagerequiredText.SelectToken("HomeKey").ToString()</a></li>
        <li><a href="#templatemo_about" class="aboutsus"><i class="glyphicon glyphicon-briefcase"></i>&nbsp; @objLanguagerequiredText.SelectToken("AboutKey").ToString()</a></li>
        <li><a href="#templatemo_events" class="events"><i class="glyphicon glyphicon-bullhorn"></i>&nbsp; @objLanguagerequiredText.SelectToken("ServicesKey").ToString()</a></li>
        <li><a href="#templatemo_timeline" class="timeline"><i class="glyphicon glyphicon-calendar"></i>&nbsp; @objLanguagerequiredText.SelectToken("TimelineKey").ToString()</a></li>
        <li><a rel="nofollow" href="#" class="external-link"><i class="glyphicon glyphicon-export"></i>&nbsp; @objLanguagerequiredText.SelectToken("Loginkey").ToString()</a></li>
        <li><a  class="contactus"><i class="glyphicon glyphicon-phone-alt"></i>&nbsp; @objLanguagerequiredText.SelectToken("ContactKey").ToString()</a></li>
    </ul>
</div>
<div class="container_wapper">
    <div id="templatemo_banner_menu">
        <div class="container-fluid">
            <div class="col-xs-4 templatemo_logo">
                <a href="#"  class="homepage">
                    <img src="../images/templatemo_logo.jpg" id="logo_img" alt="SoftDebugger" title="SoftDebugger" />
                    @*<h1 id="logo_text">Soft-<span>Debugger</span></h1>*@
                </a>
            </div>
            <div class="col-sm-8 hidden-xs">
                <ul class="nav nav-justified">
                    <li><a  class="homepage">@objLanguagerequiredText.SelectToken("HomeKey").ToString()</a></li>
                    <li><a href="#templatemo_about" class="aboutsus">@objLanguagerequiredText.SelectToken("AboutKey").ToString()</a></li>
                    <li><a href="#templatemo_events" class="events">@objLanguagerequiredText.SelectToken("ServicesKey").ToString()</a></li>
                    <li><a href="#templatemo_timeline"  class="timeline">@objLanguagerequiredText.SelectToken("TimelineKey").ToString()</a></li>
                    <li><a rel="nofollow" href="#" class="external-link">@objLanguagerequiredText.SelectToken("Loginkey").ToString()</a></li>
                    <li><a  class="contactus">@objLanguagerequiredText.SelectToken("ContactKey").ToString()</a></li>
                    
                </ul>
              
            </div>
            <div class="col-xs-8 visible-xs">
                <a href="#" id="mobile_menu"><span class="glyphicon glyphicon-th-list"></span></a>
            </div>
        </div>
    </div>
</div>
